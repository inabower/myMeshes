FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (20 0 0);

boundaryField
{
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           $internalField;
    }
    
    inlet
	{
		type  fixedValue;
		value $internalField;
	}

    sideWall
    {
        type            slip;
    }

    wall_ball
    {
        type            noSlip;
    }

    "cyclicAMI.*"
    {
        type            cyclicAMI;
    }
}


// ************************************************************************* //
